class Solution {
public:
    int findMin(vector<int>& arr) {
        int start=0,end=arr.size()-1;
        int mid;
        int ans=arr[0];
        while(start<=end)
        {
            mid=start+(end-start)/2;
            if(arr[0]<=arr[mid])
            {
                start=mid+1;
            }
            else
            {
                ans=arr[mid];
                end=mid-1;
            }
        }
        return ans;
    }
};
